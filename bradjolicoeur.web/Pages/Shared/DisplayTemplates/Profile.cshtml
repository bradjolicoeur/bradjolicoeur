@model Profile


    <div class="col-sm-2 col-xs-4">
        @if (Model.Image.Count() > 0)
        {
            <img src="@Model.Image.FirstOrDefault().Url" class="img-responsive" alt="@Model.Image.FirstOrDefault()?.Description" />
        }
    </div>
    <div class="col-sm-10 col-xs-8">
        @Html.DisplayFor(vm => vm.Bio)
    </div>
